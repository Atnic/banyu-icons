{
  "name": "@muhamien/jala-icons",
  "version": "1.0.0-Beta",
  "description": "Jala icons set for React",
  "keywords": [
    "React",
    "SVG",
    "Icons",
    "JalaIcons"
  ],
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf src && rimraf dist",
    "build": "npm run svgr && rollup -c",
    "svgr": "svgr --index-template jala-template.js --icon --title-prop --replace-attr-values '#1D1D1D=currentColor' -d src -- assets"
  },
  "author": "jala.tech",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@svgr/cli": "^8.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "rollup-plugin-filesize": "^10.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.5"
  }
}
