{
  "name": "@jala-banyu/icons",
  "version": "1.0.1",
  "description": "Banyu icons set for React by JALA",
  "keywords": [
    "React",
    "SVG",
    "Icons",
    "Banyu Icons",
    "JALA Tech"
  ],
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf src && rimraf dist",
    "build": "npm run svgr && tsup src --dts --minify terser",
    "svgr": "svgr --index-template jala-template.js --typescript --icon --title-prop --replace-attr-values \"#101828={currentColor}\" --svg-props fill=\"currentColor\" -d src -- assets"
  },
  "author": "Muhammad Amien <muhammadamien23@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/react": "^18.2.56",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "terser": "^5.27.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.5",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
